universe                = docker 
executable              = ProcessHadd.sh
arguments               = $(arg1) $(arg2) $(arg3)
transfer_input_files    = processHadd.C,ProcessHadd.py 

+WantDocker = True
docker_image ="unlhcc/osg-wn-el7"
use_x509userproxy = true

should_transfer_files   = YES
when_to_transfer_output = ON_EXIT
error                   = logs/error.$(Process).log
output                  = logs/output.$(Process).log
log                     = logs/foo.$(Process).log
queue  
